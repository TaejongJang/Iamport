{
    "name": "allivcorp/iamport",
    "description": "I'mport; REST API module for Laravel",
    "keywords": [
        "laravel",
        "iamport"
    ],
    "homepage": "https://github.com/allivcorp/iamport",
    "license": "MIT",
    "authors": [
        {
            "name": "Daeyeol Ryu",
            "email": "dy.ryu@alliv.co.kr"
        },
        {
            "name": "Jinman Lee",
            "email": "jinman.lee@alliv.co.kr"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Alliv\\Iamport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4:": {
            "Alliv\\Iamport\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alliv\\Iamport\\IamportServiceProvider"
            ],
            "aliases": {
                "Iamport": "Alliv\\Iamport\\Facades\\IamportFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
